{
  "name": "yolov7",
  "displayName": "YOLOv7",
  "version": "0.0.1",
  "scope": "public",
  "description": "YOLOv7 is a real-time object detection model optimized for efficiency and accuracy in tasks like detection and segmentation.",
  "attributes": {
    "Provider": "Ultralytics",
    "License": "AGPL 3.0",
    "Category": "Model",
    "Computer Vision": "Object Detection",
    "Media Type": [
      "Image",
      "Video"
    ],
    "Deployed By": "Dataloop"
  },
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/ultralytics-adapters.git",
    "gitTag": "0.0.1"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "yolov7-deploy",
        "runtime": {
          "podType": "regular-xs",
          "concurrency": 1,
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 100
          }
        }
      },
      {
        "name": "yolov7-train-evaluate",
        "runtime": {
          "podType": "gpu-t4",
          "concurrency": 1,
          "runnerImage": "gcr.io/viewo-g/piper/agent/gpu/yolov7:0.0.1",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 100
          }
        },
        "executionTimeout": 36000000
      }
    ],
    "modules": [
      {
        "name": "yolov7-module",
        "entryPoint": "model_adapter.py",
        "className": "Adapter",
        "computeConfig": "yolov7-deploy",
        "description": "Yolov7 Module",
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "evaluate_model",
            "computeConfig": "yolov7-train-evaluate",
            "input": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              },
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "Dataloop Dataset Entity"
              },
              {
                "type": "Json",
                "name": "filters",
                "description": "Filter to select items over which to run evaluation"
              }
            ],
            "output": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              },
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "Dataloop Dataset Entity"
              }
            ],
            "displayName": "Evaluate a Model",
            "displayIcon": "",
            "description": "Function to evaluate YOLOv7 model performance"
          },
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "List of items to run inference on"
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The same input images for prediction."
              },
              {
                "type": "Annotation[]",
                "name": "annotations",
                "description": "The predicted annotations."
              }
            ],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "Function to run YOLOv7 inference on items"
          },
          {
            "name": "predict_dataset",
            "input": [
              {
                "type": "Dataset",
                "name": "dataset",
                "description": ""
              },
              {
                "type": "Json",
                "name": "filters",
                "description": "Dataloop Filter DQL"
              }
            ],
            "output": [],
            "displayName": "Predict Dataset",
            "displayIcon": "",
            "description": "Function to run YOLOv7 inference on a dataset."
          },
          {
            "name": "train_model",
            "computeConfig": "yolov7-train-evaluate",
            "input": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              }
            ],
            "output": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              }
            ],
            "displayName": "Train a Model",
            "displayIcon": "",
            "description": "Function to train YOLOv7 model"
          }
        ]
      }
    ],
    "models": [
      {
        "name": "yolov7-Object-Detection",
        "moduleName": "yolov7-seg-module",
        "scope": "project",
        "status": "pre-trained",
        "configuration": {
          "weights_filename": "yolov7.pt",
          "checkpoint_url": "https://storage.googleapis.com/model-mgmt-snapshots/yolo-v7/yolov7-seg.pt",
          "batch_size": 2,
          "imgsz_w": 1280,
          "imgsz_h": 1280,
          "conf_thres": 0.25,
          "iou_thres": 0.45,
          "max_det": 1000,
          "augment": false,
          "amp": false,
          "freeze": 10
        },
        "inputType": "image",
        "outputType": "segment",
        "description": "The YOLO v7 algorithm outperforms its predecessors in both speed and object detection accuracy. It requires significantly cheaper hardware compared to other neural networks. YOLO v7 features faster training times, even on smaller datasets, without the need for pre-trained weights.",
