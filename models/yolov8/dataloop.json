{
  "name": "yolov8",
  "displayName": "YOLOv8",
  "version": "0.0.1",
  "scope": "public",
  "description": "An object detection model by Ultralytics that builds on previous YOLO architectures, offering improved speed, accuracy, and ease of deployment for real-time applications.",
  "attributes": {
    "Provider": "Ultralytics",
    "License": "AGPL 3.0",
    "Category": "Model",
    "Computer Vision": "Object Detection",
    "Media Type": [
      "Image",
      "Video"
    ],
    "Deployed By": "Dataloop"
  },
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/ultralytics-adapters.git",
    "gitTag": "0.0.1"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "yolov8-deploy",
        "runtime": {
          "podType": "regular-xs",
          "concurrency": 1,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/apps/ultralytics-adapters:0.0.4",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 100
          }
        }
      },
      {
        "name": "yolov8-train-evaluate",
        "runtime": {
          "podType": "gpu-t4",
          "concurrency": 1,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/apps/ultralytics-adapters:0.0.4",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 100
          }
        },
        "executionTimeout": 36000000
      }
    ],
    "modules": [
      {
        "name": "yolov8-module",
        "entryPoint": "model_adapter.py",
        "className": "Adapter",
        "computeConfig": "yolov8-deploy",
        "description": "Yolov8 Module",
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "evaluate_model",
            "computeConfig": "yolov8-train-evaluate",
            "input": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              },
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "Dataloop Dataset Entity"
              },
              {
                "type": "Json",
                "name": "filters",
                "description": "Filter to select items over which to run evaluation"
              }
            ],
            "output": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              },
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "Dataloop Dataset Entity"
              }
            ],
            "displayName": "Evaluate a Model",
            "displayIcon": "",
            "description": "Function to evaluate YOLOv8 model performance"
          },
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "List of items to run inference on"
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The same input images for prediction."
              },
              {
                "type": "Annotation[]",
                "name": "annotations",
                "description": "The predicted annotations."
              }
            ],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "Function to run YOLOv8 inference on items"
          },
          {
            "name": "predict_dataset",
            "input": [
              {
                "type": "Dataset",
                "name": "dataset",
                "description": ""
              },
              {
                "type": "Json",
                "name": "filters",
                "description": "Dataloop Filter DQL"
              }
            ],
            "output": [],
            "displayName": "Predict Dataset",
            "displayIcon": "",
            "description": "Function to run YOLOv8 inference on a dataset."
          },
          {
            "name": "train_model",
            "computeConfig": "yolov8-train-evaluate",
            "input": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              }
            ],
            "output": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              }
            ],
            "displayName": "Train a Model",
            "displayIcon": "",
            "description": "Function to train YOLOv8 model"
          }
        ]
      }
    ],
    "models": [
      {
        "name": "yolov8-Object-Detection",
        "moduleName": "yolov8-module",
        "scope": "project",
        "status": "pre-trained",
        "configuration": {
          "weights_filename": "yolov8m.pt",
          "batch_size": 4,
          "train_configs": {
            "epochs": 100,
            "batch_size": 16,
            "cache": false,
            "optimizer": "auto",
            "seed": 0,
            "deterministic": true,
            "single_cls": false,
            "classes": null,
            "rect": false,
            "cos_lr": false,
            "close_mosaic": 10,
            "fraction": 1.0,
            "lr0": 0.01,
            "lrf": 0.01,
            "weight_decay": 0.005,
            "warmup_epochs": 3.0,
            "box": 7.5,
            "cls": 0.5,
            "dfl": 1.5,
            "overlap_mask": true,
            "amp": false,
            "freeze": 10,
            "start_epoch": 0,
            "patience": 100,
            "imgsz": 640
          },
          "predict_configs": {
            "conf_thres": 0.25,
            "iou": 0.7,
            "half": false,
            "max_det": 300,
            "vid_stride": 1,
            "augment": false,
            "agnostic_nms": false,
            "classes": null,
            "imgsz": 640
          },
          "tracker_configs": {
            "tracker_type": "botsort",
            "track_high_thresh": 0.25,
            "track_low_thresh": 0.1,
            "new_track_thresh": 0.5,
            "track_buffer": 30,
            "match_thresh": 0.8,
            "include_untracked": false
          },
          "yaml_configs": {
            "hsv_h": 0.05,
            "hsv_s": 0.5,
            "hsv_v": 0.6,
            "degrees": 90.0,
            "translate": 0.2,
            "scale": 0.8,
            "shear": 20.0,
            "perspective": 0.0005,
            "flipud": 0.3,
            "fliplr": 0.7,
            "bgr": 0.2,
            "mosaic": 0.8,
            "mixup": 0.3,
            "copy_paste": 0.5,
            "copy_paste_mode": "mixup",
            "auto_augment": "augmix",
            "erasing": 0.6,
            "crop_fraction": 0.7
          }
        },
        "inputType": "image",
        "outputType": "box",
        "description": "Ultralytics YOLOv8 for Object Detection and Tracking tasks.",
